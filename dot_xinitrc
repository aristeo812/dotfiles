DISTRIBUTION_IDENTITY=$(lsb_release --id --short 2>/dev/null)
export DISTRIBUTION_IDENTITY

xsettingsd &

[ -f ~/.Xresources ] && xrdb -merge -I$HOME ~/.Xresources

# Launching DE/WM of choice
exec dbus-launch --exit-with-session openbox-session

